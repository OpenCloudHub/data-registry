{
	"dataset": {
		"name": "radiology-mini",
		"description": "Medical radiology images with captions for VLM fine-tuning",
		"source": "unsloth/Radiology_mini",
		"format": "Qwen conversation format"
	},
	"prompt": {
		"prompt_name": "radiology-vlm-prompt",
		"prompt_version": 2,
		"prompt_uri": "prompts:/radiology-vlm-prompt/2",
		"prompt_text": "You are an expert radiographer. Describe accurately what you see in this image."
	},
	"splits": {
		"train": {
			"num_samples": 400
		},
		"test": {
			"num_samples": 100
		}
	},
	"metrics": {
		"train": {
			"caption_length_mean": 20.68,
			"caption_length_min": 1,
			"caption_length_max": 98,
			"image_width_mean": 630.31,
			"image_height_mean": 574.97
		},
		"test": {
			"caption_length_mean": 21.25,
			"caption_length_min": 3,
			"caption_length_max": 76,
			"image_width_mean": 653.55,
			"image_height_mean": 590.58
		}
	}
}
